#include<iostream>
using namespace std;
void swap(int &a,int &b)
{
    a=a-b;
    b=a+b;
    a=b-a;
    cout<<"number after swapping is: "<<a<<" "<<b<<endl;
}
int main()
{
    int m,n;
    cout<<"Enter two numbers: "<<endl;
    cin>>m>>n;
    cout<<"Number before swapping is: "<<m<<" "<<n<<endl;
    swap(m,n);
return 0;
}
